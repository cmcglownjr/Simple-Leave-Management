{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db;Cache=Shared"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
      "Name": "File",
      "Args": {
        "path": "./logs/log-.log",
        "rollingInterval": "Day"
      }
    }]
  },
  "AllowedHosts": "*"
}
